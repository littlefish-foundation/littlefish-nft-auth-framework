{
  "name": "littlefish-nft-auth-framework-beta",
  "version": "0.1.44",
  "description": "Open Source NFT & Wallet Auth Framework for Cardano",
  "main": "dist/backend/index.js",
  "browser": "dist/frontend/index.js",
  "files": [
    "dist/**/**",
    "README.md"
  ],
  "exports": {
    ".": "./dist/frontend/index.js",
    "./backend": {
      "import": "./dist/backend/index.js",
      "types": "./dist/backend/index.d.ts"
    },
    "./frontend": {
      "import": "./dist/frontend/index.js",
      "types": "./dist/frontend/index.d.ts"
    }
  },
  "scripts": {
    "build:backend": "babel src/backend --out-dir dist/backend --extensions \".ts,.tsx\" --ignore \"**/*.d.ts\"\"**/*.test.ts\",\"**/*.test.tsx\",\"**/__tests__/*\" && tsc --project ./tsconfig.json",
    "build:frontend": "babel src/frontend --out-dir dist/frontend --extensions \".ts,.tsx\" --ignore \"**/*.d.ts\"\"**/*.test.ts\",\"**/*.test.tsx\",\"**/__tests__/*\" && tsc --project ./tsconfig.json",
    "build": "npm run build:backend && npm run build:frontend",
    "test": "npx jest",
    "prepublish": "npm run build"
  },  
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json",
          "ts-jest": {
            "tsconfig": "tsconfig.json"
          }
        }
      ]
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/littlefish-foundation/littlefish-nft-auth-framework"
  },
  "keywords": [
    "react",
    "wallet",
    "cardano",
    "blockchain",
    "nft",
    "auth"
  ],
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "dependencies": {
    "@cardano-foundation/cardano-verify-datasignature": "^1.0.11",
    "@prisma/client": "^5.13.0",
    "@types/jest": "^29.5.12",
    "bcryptjs": "^2.4.3",
    "cbor-js": "^0.1.0",
    "crypto": "^1.0.1",
    "prisma": "^5.13.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.3",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.24.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cbor-js": "^0.1.1",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "i": "^0.3.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm": "^10.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "rules": {
          "typescript-eslint/rule-name": "error"
        }
      }
    ]
  },
  "author": "Littlefish Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/littlefish-foundation/littlefish-nft-auth-framework/issues"
  },
  "homepage": "https://github.com/littlefish-foundation/littlefish-nft-auth-framework#readme"
}